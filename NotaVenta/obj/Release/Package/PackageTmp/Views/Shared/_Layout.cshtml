<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />

    <title>@ViewBag.Title - TexsaHub</title>
    <!-- ================== BEGIN BASE CSS STYLE ================== -->
    <!-- CSS -->
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/alertifyjs@1.11.4/build/css/alertify.min.css" />
    <!-- Default theme -->
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/alertifyjs@1.11.4/build/css/themes/default.min.css" />
    <!-- Semantic UI theme -->
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/alertifyjs@1.11.4/build/css/themes/semantic.min.css" />
    <!-- Bootstrap theme -->
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/alertifyjs@1.11.4/build/css/themes/bootstrap.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
    <link href="~/Content/assets/plugins/jquery-ui/jquery-ui.min.css" rel="stylesheet" />

    <link href="~/Content/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/assets/plugins/jquery/jquery-3.3.1.min.js"></script>
    <script src="~/Content/popper.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">

    <link href="~/Content/assets/plugins/font-awesome/css/all.min.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/animate/animate.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/default/style.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/default/style-responsive.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/default/theme/default.css" rel="stylesheet" id="theme" />
    <link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="https://cdn.datatables.net/buttons/1.5.6/css/buttons.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="https://cdn.datatables.net/colreorder/1.5.1/css/colReorder.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="https://cdn.datatables.net/fixedheader/3.1.5/css/fixedHeader.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/assets/plugins/gritter/css/jquery.gritter.css" rel="stylesheet" />
    <script type="text/javascript" src="~/Content/js/bootstrap-multiselect.js"></script>
    <link rel="stylesheet" href="~/Content/css/bootstrap-multiselect.css" />

    <!-- ================== END BASE CSS STYLE ================== -->
    <!-- ================== BEGIN PAGE LEVEL STYLE ================== -->

    <link href="~/Content/assets/plugins/DataTables/media/css/dataTables.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/DataTables/extensions/Buttons/css/buttons.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/assets/plugins/DataTables/extensions/Responsive/css/responsive.bootstrap.min.css" rel="stylesheet" />
    <!-- ================== END PAGE LEVEL STYLE ================== -->
    <!-- ================== BEGIN BASE JS ================== -->



    <script src="~/Content/assets/plugins/pace/pace.min.js"></script>

    <!-- ================== END BASE JS ================== -->

    <style>

        .loading-ajax {
            background: rgba(0,0,0,.2) url('/images/ajax-loader.gif') no-repeat 50% 50%;
            width: 100%;
            height: 100%;
            position: fixed;
            top: 0;
            left: 0;
            z-index: 999;
        }

        .loading-ajax-container {
            background-color: gray;
            color: white;
            width: 50%;
            padding: 20px;
            margin: 20px;
        }
    </style>
</head>
<body>
    <div id="loading-ajax" class="loading-ajax text-center" style="display:none">
        <div class="col-md-3 col-sm-3 col-xs-12"></div>
        <div id="loading-ajax-container" class="col-md-6 col-sm-6 col-xs-12 loading-ajax-container">
            <div class="spinner-border" role="status">
                <h4 id="loading-ajax-texto" class="alert-heading"></h4>
            </div>
        </div>
        <div class="col-md-3 col-sm-3 col-xs-12"></div>
    </div>
    <!-- begin #page-loader -->
    <div id="page-loader" class="fade show"><span class="spinner"></span></div>
    <!-- end #page-loader -->
    <!-- begin #page-container -->
    <div id="page-container" class="fade page-sidebar-fixed page-header-fixed page-with-wide-sidebar">
        <!-- begin #header -->
        <div id="header" class="header navbar-default ">
            <!-- begin navbar-header -->
            <div class="navbar-header">
                <a href="@Url.Action("Index", "Home")" class="navbar-brand"><span class="navbar-logo"></span><b>Texsa HUB</b></a>
                <button type="button" class="navbar-toggle" data-click="sidebar-toggled">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <!-- end navbar-header -->
            <!-- begin header-nav -->
            <ul class="navbar-nav navbar-right">
                <li class="dropdown navbar-user">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        <img src="~/Content/assets/img/user/TexsaHub.png" alt="" />
                        <span class="d-none d-md-inline">Texsa HUB</span> <b class="caret"></b>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <div class="dropdown-divider"></div>
                        <a href="@Url.Action("LogOut", "Login")" class="dropdown-item">Cerrar Sesion</a>
                    </div>
                </li>
            </ul>
            <!-- end header navigation right -->
        </div>
        <!-- end #header -->
        <!-- begin #sidebar -->

        <div id="sidebar" class="sidebar">
            <!-- begin sidebar scrollbar -->
            <div data-scrollbar="true" data-height="100%">
                <!-- begin sidebar user -->
                <ul class="nav">
                    <li class="nav-profile">
                        <a href="javascript:;" data-toggle="nav-profile">
                            <div class="cover with-shadow"></div>
                            <div class="image">
                                <img src="~/Content/assets/img/user/TexsaHub.png" alt="" />
                            </div>
                            <div class="info">
                                <b class=""></b>
                                Texsa HUB
                            </div>
                        </a>
                    </li>
                </ul>
                <!-- end sidebar user -->
                <!-- begin sidebar nav -->
                @using TexsaHub.Helper
                @Html.Menu()
                <!-- end sidebar nav -->
            </div>
            <!-- end sidebar scrollbar -->
        </div>
        <a href="javascript:;" class="btn btn-icon btn-circle btn-success btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
        <!-- end scroll to top btn -->
        <div id="content" class="content">
            @RenderBody()
        </div>
    </div>


    <input id="btnModalError" type="button" style="display: none" data-toggle="modal" data-target="#modalError" />
    <div id="modalError" class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content alert alert-danger">
                <div class="modal-header">
                    <h5 class="modal-title" id="modalErrorTitulo"></h5>
                    <button id="modalErrorCerrar" type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p id="modalErrorMensaje"></p>
                </div>
                <div class="modal-footer">
                    <button type="button" id="modalErrorAceptar" class="btn btn-primary">Aceptar</button>
                </div>
            </div>
        </div>
    </div>

    <input id="btnModalInformacion" type="button" style="display: none" data-toggle="modal" data-target="#modalInformacion" />
    <div id="modalInformacion" class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modalInformacionTitulo"></h5>
                    <button id="modalInformacionCerrar" type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p id="modalInformacionMensaje"></p>
                </div>
                <div class="modal-footer">
                    <button type="button" id="modalInformacionAceptar" class="btn btn-primary">Aceptar</button>
                </div>
            </div>
        </div>
    </div>

    <input id="btnModalConfirmacion" type="button" style="display: none" data-toggle="modal" data-target="#modalConfirmacion" />
    <div id="modalConfirmacion" class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modalConfirmacionTitulo"></h5>
                    <button id="modalConfirmacionCerrar" type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p id="modalConfirmacionMensaje"></p>
                </div>
                <div class="modal-footer">
                    <button type="button" id="modalConfirmacionAceptar" class="btn btn-primary">Aceptar</button>
                    <button type="button" id="modalConfirmacionCancelar" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                </div>
            </div>
        </div>
    </div>
    <!-- end page container -->
    <!-- ================== BEGIN BASE JS ================== -->


    <script src="~/Content/assets/plugins/jquery-ui/jquery-ui.min.js"></script>

    <script src="~/Content/assets/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/assets/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <script src="~/Content/assets/plugins/js-cookie/js.cookie.js"></script>
    <script src="~/Content/assets/js/theme/default.min.js"></script>
    <script src="~/Content/assets/js/apps.min.js"></script>
    <script src="//cdn.jsdelivr.net/npm/alertifyjs@1.11.4/build/alertify.min.js"></script>
    <script src="~/Content/assets/plugins/gritter/js/jquery.gritter.js"></script>
    <script src="~/Content/assets/plugins/bootstrap-sweetalert/sweetalert.min.js"></script>
    <script src="~/Content/assets/js/demo/ui-modal-notification.demo.min.js"></script>
    <script src="~/WebJs/Base.js"></script>




    <script type="text/javascript" src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.6/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.5.6/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/colreorder/1.5.1/js/dataTables.colReorder.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/fixedheader/3.1.5/js/dataTables.fixedHeader.min.js"></script>
    <!-- ================== END BASE JS ================== -->
    <script>
        $(document).ready(function () {
            $('#tblNomina').DataTable({
                dom: 'Bfrtip',
                buttons: [
                    'csv', 'excel'
                ],
                ordering: false,
                deferRender: true
            });
            $('#multiselect').multiselect({
                buttonWidth: '160px',
                includeSelectAllOption: true,
                nonSelectedText: 'Seleccione Cuenta',
                enableFiltering: true,
                maxHeight: '450'
            });
            $('#BaseDatos').multiselect();
        });
    </script>
    <script>
        $(document).ready(function () {
            App.init();
            Notification.init();
        });
    </script>
    <script src="~/WebJs/Prueba.js"></script>
</body>

</html>
